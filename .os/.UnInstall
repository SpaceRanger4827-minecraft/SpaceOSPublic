term.setBackgroundColor( colors.red )
term.clear()
term.setCursorPos( 1,1 )
print( "Are you sure about this?" )
A = read()
if A == "y" then
  term.clear()
  term.setCursorPos( 1,1 )
  print( "UnInstalling" )
  sleep(1)
  print( "Removing All System Settings + Files" )
  fs.delete( "/.os/" )
  fs.delete( "/startup" )
  fs.delete( "/.settings" )
  fs.delete( "edit" )
  sleep(2)
  fs.delete( "/SpaceOS-Installer" )
  print( "System Files Removed Creating Installer" )
  shell.run( "pastebin get LqKwPb6N SpaceOS-Installer" )
  sleep(5)
  os.reboot()
else
  print( "UnInstallation Failed. ReBooting" )
  sleep(5)
  shell.run( "/.os/.gui" )
end